#include <stdio.h>
#include <stdlib.h>
void simulateCache(int arraySize) {
     for (int i = 0; i < arraySize; ++i) {
         }
}
int main() {
    printf("Simulating cache behavior...\n");
    // Simulate cache behavior with different array sizes
